# Text normalization
* text=auto eol=lf

# Treat everything in assets as binary by default
assets/** -text

# Large/binary assets
assets/**/*.onnx filter=lfs diff=lfs merge=lfs -text
assets/**/*.yuv  filter=lfs diff=lfs merge=lfs merge=lfs -text

# Images
assets/**/*.png  filter=lfs diff=lfs merge=lfs -text
assets/**/*.jpg  filter=lfs diff=lfs merge=lfs -text
assets/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
